.container {
  display: flex;
  flex-direction: column;
  height: 100vh;
  background-color: #f5f5f5;
}

.page-header {
  background-color: #FF9500;
  padding: 30rpx;
  color: #ffffff;
}

.page-title {
  font-size: 36rpx;
  font-weight: bold;
  text-align: center;
}

.page-content {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 30rpx;
}

.tip-message {
  font-size: 32rpx;
  color: #666666;
  text-align: center;
} 